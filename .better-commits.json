        "append_emoji_to_label": true,
        "append_emoji_to_commit": true,
        "options": [
            {
                "value": "feat",
                "label": "feat",
                "hint": "A new feature",
                "emoji": "‚ú®",
                "trailer": "Changelog: feature"
            },
            {
                "value": "fix",
                "label": "fix",
                "hint": "A bug fix",
                "emoji": "üêõ",
                "trailer": "Changelog: fix"
            },
            {
                "value": "refactor",
                "label": "refactor",
