                "emoji": "üî®",
                "trailer": "Changelog: refactor"
            },
            {
                "value": "perf",
                "label": "perf",
                "hint": "A code change that improves performance",
                "emoji": "üöÄ",
                "trailer": "Changelog: performance"
            },
            {
                "value": "chore",
                "label": "chore",
                "hint": "Other changes that do not modify src or test files",
                "emoji": "üßπ",
                "trailer": "Changelog: chore"
            },
            {
                "value": "",
                "label": "none"
            }
        ]
    },
    "commit_scope": {
        "enable": true,
        "custom_scope": false,
        "max_items": 20,
        "initial_value": "",
        "options": [
            { "value": "blink", "label": "blink" },
            { "value": "codecompanion", "label": "codecompanion" },
            { "value": "conform", "label": "conform" },
            { "value": "defaults", "label": "defaults" },
            { "value": "git", "label": "git" },
            { "value": "languages", "label": "languages" },
            { "value": "lazy", "label": "lazy" },
            { "value": "highlights", "label": "highlights" },
            { "value": "lsp", "label": "lsp" },
            { "value": "options", "label": "options" },
            { "value": "noice", "label": "noice" },
            { "value": "nvim-lint", "label": "nvim-lint" },
            { "value": "sessions", "label": "sessions" },
            { "value": "snacks", "label": "snacks" },
            { "value": "treesitter", "label": "treesitter" },
            { "value": "", "label": "none" }
        ]
    },
    "check_ticket": {
        "infer_ticket": true,
        "confirm_ticket": false,
        "add_to_title": false,
        "append_hashtag": false,
        "prepend_hashtag": "Never",
        "surround": "",
        "title_position": "start"
    },
    "commit_title": {
        "max_size": 80
    },
    "commit_body": {
        "enable": true,
        "required": false
    },
    "commit_footer": {
        "enable": false,
        "initial_value": [],
        "options": [
            "closes",
            "trailer",
            "breaking-change",
            "deprecated",
            "custom"
        ]
    },
    "breaking_change": {
        "add_exclamation_to_title": false
    },
    "confirm_with_editor": false,
    "confirm_commit": true,
    "print_commit_output": true,
    "branch_pre_commands": [],
    "branch_post_commands": [],
    "worktree_pre_commands": [],
    "worktree_post_commands": [],
    "branch_user": {
        "enable": true,
        "required": false,
        "separator": "/"
    },
    "branch_type": {
        "enable": true,
        "separator": "/"
    },
    "branch_version": {
        "enable": false,
        "required": false,
        "separator": "/"
    },
    "branch_ticket": {
        "enable": true,
        "required": false,
        "separator": "-"
    },
    "branch_description": {
        "max_length": 70,
        "separator": ""
    },
    "branch_action_default": "branch",
    "branch_order": [
        "user",
        "version",
        "type",
        "ticket",
        "description"
    ],
    "enable_worktrees": true,
    "overrides": {}
