{
    "$schema": "https://raw.githubusercontent.com/EmmyLuaLs/emmylua-analyzer-rust/refs/heads/main/crates/emmylua_code_analysis/resources/schema.json",
    "diagnostics": {
        "disable": [
            "missing-fields",
            "type-not-found",
            "undefined-field"
        ],
        "globals": [
            "Snacks",
            "bit",
            "colors",
            "defaults",
            "ev",
            "hl",
            "keys",
            "ns",
            "package",
            "require",
            "vim"
        ],
        "unusedLocalExclude": [
            "_*"
        ]
    },
    "runtime": {
        "version": "LuaJIT"
    },
    "workspace": {
        "enableReindex": true,
        "library": [
